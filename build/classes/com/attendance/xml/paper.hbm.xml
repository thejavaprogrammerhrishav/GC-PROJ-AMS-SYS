<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.attendance.papers.model.Paper" table="papers" catalog="attendance">
      <id column="id" name="id">
          <generator class="identity"/>
      </id>
    <property column="code" name="paperCode" not-null="true"/>
    <property column="name" name="paperName" not-null="true"/>
    <property column="semester" name="semester" not-null="true"/>
     <property column="department" name="department" not-null="true"/>
      <property column="coursetype" name="coursetype" not-null="true"/>
  </class>
</hibernate-mapping>

