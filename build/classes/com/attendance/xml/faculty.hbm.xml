<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.attendance.faculty.model.Faculty" table="faculty" catalog="attendance">
    <id column="contact" name="contact"/>
    <property column="firstName" name="firstName" not-null="true"/>
    <property column="lastName" name="lastName" not-null="true"/>
    <property column="emailId" name="emailId" not-null="true"/>
    <property column="gender" name="gender" not-null="true"/>
    <property column="department" name="department" not-null="true"/>
  </class>
</hibernate-mapping>
