<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.attendance.personal.model.PersonalDetails" table="personaldetails" catalog="attendance">
      <id column="id" name="id">
          <generator class="identity"/>
      </id>
    <property column="name" name="name" not-null="true"/>
    <property column="emailId" name="emailId" not-null="true"/>
    <property column="gender" name="gender" not-null="true"/>
    <property column="contact" name="contact" not-null="true"/>
  </class>
</hibernate-mapping>
