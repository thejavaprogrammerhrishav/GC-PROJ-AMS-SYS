<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 
<hibernate-mapping>
    <class name="com.attendance.login.user.model.SecurityQuestion" table = "security" catalog="attendance">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="question1" column="question1" not-null="true"/>
        <property name="question2" column="question2" not-null="true"/>
        <property name="question3" column="question3" not-null="true"/>
        <property name="answer1" column="answer1" not-null="true"/>
        <property name="answer2" column="answer2" not-null="true"/>
        <property name="answer3" column="answer3" not-null="true"/>
    </class>
</hibernate-mapping>
