<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                            "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.attendance.login.user.model.User" table="loginuser" catalog="attendance">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="contact" column="contact" not-null="true"/>
        <property name="username" column="username" not-null="true"/>
        <property name="password" column="password" not-null="true"/>
        <property name="type" column="type" not-null="true"/>
        <property name="image" not-null="true">
            <column name="image" sql-type="longblob"/>
        </property>
    </class>
</hibernate-mapping>
