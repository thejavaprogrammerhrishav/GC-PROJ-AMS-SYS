<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.attendance.marks.model.UnitTest" table="unittest" catalog="attendance">
      <id column="id" name="id">
          <generator class="identity"/>
      </id>
    <property column="name" name="name" not-null="true"/>
    <property column="rollno" name="rollno" not-null="true"/>
    <property column="marksobtained" name="marksObtained" not-null="true"/>
    <property column="passingmarks" name="passingMarks" not-null="true"/>
    <property column="totalMarks" name="totalMarks" not-null="true"/>
    <property column="unittestcode" name="unitTest" not-null="true"/>
    <property column="acadamicyear" name="acadamicYear" not-null="true"/>
    <property column="semester" name="semester" not-null="true"/>
    <property column="year" name="year" not-null="true"/>
  </class>
</hibernate-mapping>
